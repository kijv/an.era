{
  "name": "an.era",
  "version": "0.3.0-beta.1",
  "description": "Interfacing for Are.na API",
  "keywords": [
    "are.na"
  ],
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kijv/an.era.git"
  },
  "workspaces": [
